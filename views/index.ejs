<script>
    let performance = document.querySelector('#performance');
    let blob = document.querySelectorAll('.blob');

    performance.addEventListener('click', () => {
        blob.forEach((item) => {
            item.classList.toggle('hide');
        });
    });


    const copyLink = document.querySelectorAll('#copy_link');
    const tool = document.querySelectorAll('.tool');

    tool.forEach(function (item) {
        item.addEventListener('mouseover', function () {
            item.querySelector('#copy_link').classList.remove('hide');
            
        })  

        item.addEventListener('click', function () {
            const link = item.querySelector('.link').href;
            navigator.clipboard.writeText(link);
            item.querySelector('#copy_link').innerHTML = 'copied! ðŸš€';
            setTimeout(function () {
                item.querySelector('#copy_link').innerHTML = 'ðŸ”—';
            }, 1000);
        })

        item.addEventListener('mouseleave', function () {
            item.querySelector('#copy_link').classList.add('hide');
        })
    })

    if (window.location.hash) {
        var hash = window.location.hash;
    
        if (hash == '#tools') {
            window.scrollTo(0, 1000);
        }
    }

    var category = document.querySelector('.categories');
    var category_mobile = document.querySelector('.categories_mobile');
    var sticky = category.offsetTop;
    var sticky_mobile = category_mobile.offsetTop;

    function myFunction() {
        if (window.pageYOffset >= sticky) {
            category.classList.add("sticky")
        } 
        else {
            category.classList.remove("sticky");
        }

        if (window.pageYOffset >= sticky_mobile) {
            category_mobile.classList.add("sticky") 
        } 
        else {
            category_mobile.classList.remove("sticky");
        }
    }

    window.onscroll = function() {myFunction()};
</script>
